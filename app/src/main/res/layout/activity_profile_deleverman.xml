<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.uuproject2.mks.salescycle.Comp.deleverman.ProfileDelevermanActivity">

   <ImageView
       android:id="@+id/imDelever"
       android:layout_width="100dp"
       android:layout_height="128dp"
       android:src="@mipmap/delever"
       android:layout_alignTop="@+id/imDeleverLocation"
       android:layout_alignParentStart="true" />


    <ImageView
        android:id="@+id/logout"
        android:layout_width="100dp"
        android:layout_height="128dp"
        android:scaleType="fitXY"
        android:src="@mipmap/logout"

        android:layout_alignTop="@+id/imDeleverLocation"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="22dp" />
    <ImageView
        android:id="@+id/imDeleverLocation"
        android:layout_width="100dp"
        android:layout_height="128dp"
        android:src="@mipmap/locationsharing"
        android:layout_marginTop="14dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
